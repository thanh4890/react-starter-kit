{
  "name": "react-examples",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.11.0",
    "react-scripts": "^1.1.4"
  },
  "dependencies": {
    "connected-react-router": "^4.3.0",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sanitize.css": "^5.0.0"
  },
  "scripts": {
    "start": "npm-run-all -p sass:watch start-js",
    "start-js": "react-scripts start",
    "build": "npm-run-all sass:build build-js",
    "build-js": "react-scripts build",
    "sass:watch": "node-sass -w src/styles/scss -o src/styles/css",
    "sass:build": "node-sass src/styles/scss -o src/styles/css --source-map true",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "git add"
    ]
  }
}
